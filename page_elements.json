{
    "username_input": {
        "type": "xPath",
        "value": "//div[contains(@data-a-target,'login-username-input')]/input"
    },
    "password_input": {
        "type": "xPath",
        "value": "//div[contains(@data-a-target,'login-password-input')]/input"
    },
    "login_button": {
        "type": "xPath",
        "value": "//button[contains(@data-a-target,'passport-login-button')]"
    },
    "my_account": {
        "type": "xPath",
        "value": "//div[contains(@data-a-target, 'user-display-name')]"
    },
    "following_account": {
        "type": "xPath",
        "value": "//div[contains(@class, 'channel-header')]"
    },
    "follow_button": {
        "type": "xPath",
        "value": "//div[contains(@data-target, 'channel-header-right')]//button[contains(@data-a-target, 'follow-button')]"
    },
    "unfollow_button": {
        "type": "xPath",
        "value": "//div[contains(@data-target, 'channel-header-right')]//button[contains(@data-a-target, 'unfollow-button')]"
    },
    "auth_failed": {
        "type": "xPath",
        "value": "//div[contains(@class, 'server-message-alert')]"
    },
    "recaptcha": {
        "type": "xPath",
        "value": "//div[contains(@id, 'recaptcha-element-container')]"
    }
}